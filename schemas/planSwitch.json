{
  "id": "plan.create",
  "type": "object",
  "required": ["data"],
  "properties": {
    "data": {
      "type": "object",
      "required": ["type", "attributes"],
      "properties": {
        "type": { "type": "string", "enum": ["switch"] },
        "attributes": {
          "type": "object",
          "required": ["user", "plan"],
          "properties": {
            "user": { "type": "string", "minLength": 1 },
            "agreement": {
              "type": "object",
              "required": ["name", "plan"],
              "properties": {
                "name": { "type": "string", "minLength": 1 },
                "description": { "type": "string" },
                "start_date": { "type": "string", "format": "date-time" },
                "plan": { "type": "string", "minLength": 1 }
              }
            }
          }
        }
      }
    }
  }
}
