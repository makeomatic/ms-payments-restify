  "bugs": {
    "url": "https://github.com/makeomatic/ms-payments-restify/issues"
  },
  "homepage": "https://github.com/makeomatic/ms-payments-restify#readme",
  "dependencies": {
    "bluebird": "^3.0.6",
    "common-errors": "latest",
    "debug": "^2.2.0",
    "glob": "^6.0.1",
    "lodash": "^3.10.1",
    "mm-lodash": "^1.2.5",
    "ms-users-restify": "^0.5.11",
    "restify-utils": "^0.3.5"
  },
  "devDependencies": {
    "babel": "^5.8.34",
    "babel-core": "^5.8.33",
    "babel-eslint": "^4.1.5",
    "bunyan": "^1.5.1",
    "chai": "^3.4.1",
    "eslint": "^1.8.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-plugin-mocha": "^1.1.0",
    "eslint-plugin-react": "^3.9.0",
    "json": "^9.0.3",
    "mocha": "^2.3.4"
  }
